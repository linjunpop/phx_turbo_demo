<turbo-frame id="todos">
  <h1>Listing Todos</h1>

  <turbo-frame id="todo_list">
    <%= for todo <- @todos do %>
      <%= render "_todo.html", conn: @conn, todo: todo %>
    <% end %>
  </turbo-frame>

  <turbo-frame id={dom_id(["new", "todo"])}>
    <span><%= link "New Todo", to: Routes.todo_path(@conn, :new) %></span>
  </turbo-frame>
</turbo-frame>